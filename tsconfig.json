{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": ".",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": ["*.ts", "routes/*.ts", "services/*.ts"],
  "exclude": ["node_modules"]
}
